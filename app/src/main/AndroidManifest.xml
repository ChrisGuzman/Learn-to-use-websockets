<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chris_guzman.learn_to_use_websockets">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".WebSocketApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OkHttpActivity" />
        <activity android:name=".SecondActivity" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PubNubActivity" />
        <activity android:name=".PusherActivity" />
        <activity android:name=".KoushActivity" />
        <activity android:name=".NateActivity"></activity>
    </application>

</manifest>